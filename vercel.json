{
  "version": 2,
  "builds": [
    {
      "src": "TCC/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "buildCommand": "sh build_files.sh",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "TCC/wsgi.py"
    }
  ]
}